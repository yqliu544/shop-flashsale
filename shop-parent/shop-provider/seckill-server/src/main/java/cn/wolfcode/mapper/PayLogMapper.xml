<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.wolfcode.mapper.PayLogMapper" >

  <insert id="insert" useGeneratedKeys="true" keyProperty="id" >
    insert into t_pay_log (trade_no,out_trade_no,notify_time,total_amount,pay_type)
            values
            (#{tradeNo},#{outTradeNo},#{notifyTime},#{totalAmount},#{payType})
  </insert>

</mapper>